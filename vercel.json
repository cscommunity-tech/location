{
	"version": 2,
	"builds": [
		{
			"src": "api/*.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/places/autocomplete",
			"dest": "/api/autocomplete.js"
		},
		{
			"src": "/places/details",
			"dest": "/api/details.js"
		},
		{
			"src": "/",
			"dest": "/api/hello.js"
		}
	],
	"functions": {
		"api/*.js": {
			"includeFiles": ["node_modules/axios/dist/**/*"]
		}
	}
}
