{
	"version": 2,
	"builds": [
		{
			"src": "api/*.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/places/autocomplete",
			"dest": "/api/places/autocomplete.js"
		},
		{
			"src": "/api/places/details",
			"dest": "/api/places/details.js"
		},
		{
			"src": "/",
			"dest": "/api/hello.js"
		}
	]
}
